{
	"name": "@twosense/n8n-nodes-twosense",
	"version": "0.1.0",
	"description": "n8n community node for Twosense continuous authentication and behavioral biometrics API",
	"license": "MIT",
	"homepage": "https://github.com/twosense/n8n-nodes-twosense#readme",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"twosense",
		"authentication",
		"biometrics",
		"security"
	],
	"author": {
		"name": "Twosense",
		"email": "support@twosense.ai"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/twosense/n8n-nodes-twosense.git"
	},
	"scripts": {
		"build": "n8n-node build && mkdir -p dist/assets && cp -R assets/* dist/assets/",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"nodes": [
			"dist/nodes/Twosense/Twosense.node.js",
			"dist/nodes/Twosense/TwosenseTrigger.node.js"
		],
		"credentials": [
			"dist/credentials/TwosenseApi.credentials.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@types/node": "^25.0.9",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
